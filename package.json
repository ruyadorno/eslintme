{
  "name": "eslintme",
  "version": "3.0.0",
  "description": "The fastest way to eslint a single file",
  "license": "MIT",
  "repository": "ruyadorno/eslintme",
  "author": {
    "name": "Ruy Adorno",
    "url": "ruyadorno.com"
  },
